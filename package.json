{
  "name": "BurstWalletAIO",
  "version": "1.1.0",
  "scripts": {
    "pack": "electron-packager . BurstWalletAIO --arch=x64 --out ./build --overwrite --icon=icons/icon.ico",
    "rename": "cd build && rename BurstWalletAIO-win32-x64 BurstWalletAIO-x64-1.1.0",
    "zip": "cd build && \"C:/Program Files/7-Zip/7z.exe\" a -tzip BurstWalletAIO-x64-1.1.0.zip \"BurstWalletAIO-x64-1.1.0\""
  },
  "main": "app/main.js",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.5.2",
    "electron-rebuild": "^1.5.7"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron-reload": "^1.1.0",
    "jquery": "^3.1.1",
    "diskusage": "0.2.1"
  }
}
